{
  "name": "@experiterm/client",
  "version": "0.0.1",
  "description": "Browser-based UI for the Experiterm daemon",
  "main": "index.js",
  "author": "Benjamin Duran <stratodyne@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "vite build",
    "lint": "eslint './src/**/*.{tsx,ts}'",
    "precommit": "lint-staged --allow-empty",
    "start": "run-p vite:start typecheck:watch",
    "typecheck:watch": "tsc --project ./tsconfig.json --noEmit --watch",
    "vite:start": "vite"
  },
  "validators": {
    "*.{ts,tsx}": [
      "@parcel/validator-typescript"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9"
  },
  "dependencies": {
    "ansi-styles": "^6.1.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "yarn lint"
  }
}
